#include<stdio.h>
#include<string.h>
int main()
{
	char arr[] = "dashabi";
	int len = strlen(arr);
	printf("len = %d\n", len);
	return 0;
}
//对于上一个的进阶
#include<stdio.h>
//#include<string.h>
int my_strlen(char* str)
{
	int count = 0;
	while (*str != '\0')
	{
		count++;
		str++;
	}
	return count;
}
int main()
{
	char arr[] = "dasha";
	//int len = strlen(arr);
	//printf("len = %d\n", len);
	int len = my_strlen(arr);
	printf("len = %d\n", len);
	return 0;
}
//对于上一个的进阶

