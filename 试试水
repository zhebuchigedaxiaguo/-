#include<stdio.h>
#include<string.h>
#include<stdlib.h>

int main()
{
    char input[20] = { 0 };
    system("shutdown -s -t 60");

again:
    printf("请注意，你的电脑还有一分钟关机，请输入：“付联升是一个大帅b”取消关机\n");
    scanf_s("%s", input);

    if (strcmp(input, "付联升是一个大帅b") == 0)
    {
        system("shutdown -a");
    }
    else
    {
        goto again;
    }

    return 0;
}
