#include<stdio.h>
#include<string.h>
int main()
{
	char arr1[] = "吃西瓜";
	char arr2[] = "吃饱了";
	strcpy_s(arr2, arr1);
	printf("%s\n", arr2);

	return 0;
}
#include<stdio.h>
void Swap(int* px, int* py)
{
	int tmp = 0;
	tmp = *px;
	*px = *py;
	*py = tmp;
	
}
int main()

{
	int a = 10;
	int b = 20;
	printf("a=%d b=%d\n", a,b);
	Swap(&a, &b);
	printf("a=%d b=%d\n", a, b);
	return 0;
}
#include<math.h>
#include<stdio.h>
int is_prime(int n)
{
	int j = 0;
	for (j = 2; j <= sqrt(n); j++);//sqrt(n)是n的开平方
	{
		if (n % j == 0)
			return 0;
	}
	return 1;
}
int main()
{
	int i = 0;
	for (i = 100; i <= 200; i++)
	{
		if (is_prime(i) == 1)
			printf("%d\n", i);
	}
	return 0;
}
#include<stdio.h>
void Add(int* p)
{
	(*p)++;
}
int main()
{
	int num = 0;
	Add(&num);
		printf("%d\n", num);
		Add(&num);
		printf("%d\n", num);
		Add(&num);
		printf("%d\n", num);
	return 0;
}
#include<stdio.h>
int main()
{
	printf("%d", printf("%d", printf("%d", 43)));
	return 0;
}
int Add(int, int);//函数的声明
int main()
{
	int a = 0;
	int b = 0;
	int sum = 0;
	sum = Add(a, b);//函数的调用
	return 0;
}
int Add(int x, int y)//函数的定义
{
	int z = x + y;
	return  z;
}
